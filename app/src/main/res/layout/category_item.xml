<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        
        <variable
            name="categoryModel"
            type="com.example.brikotlin.domain.model.main.maincategory.MainCategoryDetails" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.ConstraintLayout
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="236.5dp"
            android:layout_height="44dp">

            <TextView
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:id="@+id/right_side_item_text"
                android:layout_width="wrap_content"
                android:text="@{categoryModel.name}"
                android:layout_height="20dp"
                android:layout_marginLeft="15dp"
                android:textColor="@color/app_text_color"
                android:textSize="15dp"
                android:layout_centerVertical="true" />

            <ImageView
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_width="18dp"
                android:layout_height="18dp"
                app:srcCompat="@drawable/group_36"
                android:layout_marginRight="15dp"
                android:layout_alignParentRight="true" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>